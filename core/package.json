{
  "name": "@aukjs/core",
  "version": "0.4.1",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "CHANGELOG.md",
    "readme.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs --minify terser",
    "size": "bun run scripts/check-size.ts",
    "dev": "tsup src/index.ts --dts --format esm,cjs --watch",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.2",
    "@types/bun": "^1.2.18",
    "@types/jest": "^30.0.0",
    "terser": "^5.43.1",
    "tsup": "^8.5.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.34.38"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module"
}
